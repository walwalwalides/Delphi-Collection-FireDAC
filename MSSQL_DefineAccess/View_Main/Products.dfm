  object qrProducts: TFDQuery
    AfterOpen = qrProductsAfterOpen
    Connection = DMModule.ConnectionMain
    SQL.Strings = (
      
        'SELECT ProductID ,Typs.TypDescription,Marks.MarkDescription,Mode' +
        'l,SerialNumber,Price'
      'FROM Products, Typs, Marks'
      
        'WHERE (Products.TypID=Typs.TypID) and (Products.MarkID=Marks.Mar' +
        'kID)')
    Left = 673
    Top = 352
  end
